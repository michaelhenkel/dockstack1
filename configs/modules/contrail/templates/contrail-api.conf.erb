[DEFAULTS]
ifmap_server_ip=<%= @ipaddress %>
ifmap_server_port=<%= @ifmap_server_port %>
ifmap_username=api-server
ifmap_password=api-server
cassandra_server_list=<%= @registered_cassandra.map{ |ip| "#{ip}:#{@database_ip_port}" }.join(' ') %>
listen_ip_addr=0.0.0.0
listen_port=8082
auth=keystone
multi_tenancy=<%= @multi_tenancy %>
log_file=/var/log/contrail/api.log
log_local=1
log_level=SYS_NOTICE
disc_server_ip=<%= @vip_name %>
disc_server_port=5998
zk_server_ip=<%= @registered_cassandra.map{ |ip| "#{ip}:#{@zk_ip_port}" }.join(',') %>
rabbit_server=<%= @vip_name %>
rabbit_port=5672

[SECURITY]
use_certs=<%= @use_certs %>
keyfile=/etc/contrail/ssl/private_keys/apiserver_key.pem
certfile=/etc/contrail/ssl/certs/apiserver.pem
ca_certs=/etc/contrail/ssl/certs/ca.pem

[KEYSTONE]
auth_host=<%= @vip_name %>
auth_protocol=<%= @keystone_auth_protocol %>
auth_port=<%= @keystone_auth_port %>
admin_user=<%= @keystone_admin_user %>
admin_password=<%= @keystone_admin_password %>
admin_token=<%= @keystone_admin_token %>
admin_tenant_name=<%= @keystone_admin_tenant %>
insecure=<%= keystone_insecure_flag %>
<%= memcached_opt %>
